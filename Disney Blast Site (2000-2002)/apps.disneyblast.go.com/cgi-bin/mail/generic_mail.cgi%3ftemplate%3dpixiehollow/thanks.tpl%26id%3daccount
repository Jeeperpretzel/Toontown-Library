  function put() 
  {
    var mylist=document.getElementById("Category_Message")      
    document.getElementById("subject").value = "Pixie Hollow - " + mylist.options[mylist.selectedIndex].text;    
  }



function setDisplayText()
{
    searchParameters = new queryPal()
    checkFormId = searchParameters.checkQueryString("id")

	var varReturn = "Thanks!  We appreciate your help making Pixie Hollow more magical."
	
	switch (checkFormId){
	
		case "questions":
			varReturn = "Thanks!  We appreciate your help making Pixie Hollow more magical."
		
		break;
	
		case "nevercouncil":
			varReturn = "Thanks for leaving us a note!  We always love to hear from a Fairy friend."
		
		break;
	
		case "feebackPixieHollow":
			varReturn = "Thanks for your feedback! We appreciate your help making Pixie Hollow more magical."
		
		break;
		
		
		case "account":
			varReturn = "Thank you Fairy much! We will respond to your account question very soon.  Fly with you!"
		
		break;
		
		case "flashupgrade":
			varReturn = "Thanks!  We will respond to your issue soon.  If you need additional help, please contact Pixie Hollow Customer Support at (877) 78-PIXIE between the hours of 6 a.m. and 7 p.m PST every day of the week."
				
		break;
		
		
		case "badName":
			varReturn = "Thanks!  We appreciate your help making Pixie Hollow a Fairy-friendly place."
		
		break;
		
		case "misbehavingPlayers":
			varReturn = "Thanks!  We appreciate your help making Pixie Hollow a fun, safe, and Fairy-friendly place."
		
		break;
		
		case "chat":
			varReturn = "Thanks for your suggestion!  We appreciate your help making Pixie Hollow even more flitterific."
		
		break;
				
		case "billingProblems":
			varReturn = "Thanks!  We will respond to your issue soon.  If you need additional help, please contact Pixie Hollow Customer Support at (877) 78-PIXIE between the hours of 6 a.m. and 7 p.m PST every day of the week."
		
		break;
		
						
		case "suggestName":
			varReturn = "Thanks for your flitterific idea!  We appreciate your help making Pixie Hollow more magical."
		
		break;
	}
	
	return varReturn;
}




  function display_form()
  {
    //by camartin
    //use querypal.js to get grab formId string and select value from dropdown menu

    var mylist=document.getElementById("Category_Message")

    searchParameters = new queryPal()
    checkFormId = searchParameters.checkQueryString("formId")


    document.write('<select name="Category_Message" id="Category_Message" onChange="put()">')

      document.write('<option name="questions" id="questions" value="Question/Comment/Suggestion" selected>Question/Comment/Suggestion</option>')
      document.write('<option name="badName" id="badName" value="Report Bad Name">Report Bad Name</option>')

      if (checkFormId == "misbehavior")
      {
        document.write('<option name="misbehavingPlayers" id="misbehavingPlayers" value="Report Misbehaving Players" selected="selected">Report Misbehaving Players</option>')
      }
      else
      {
        document.write('<option name="misbehavingPlayers" id="misbehavingPlayers" value="Report Misbehaving Players">Report Misbehaving Players</option>')
      }

      if (checkFormId == "billing")
      {
        document.write('<option name="billingProblems" id="billingProblems" value="Billing Problem" selected="selected">Billing Problem</option>')
      }
      else
      {
        document.write('<option name="billingProblems" id="billingProblems" value="Billing Problem">Billing Problem</option>')
      }

      if (checkFormId == "feedback")
      {
        document.write('<option name="feebackPixieHollow" id="feebackPixieHollow" value="Feedback from Pixie Hollow" selected="selected">Feedback from Pixie Hollow</option>')
      }
      else
      {
        document.write('<option name="feebackPixieHollow" id="feebackPixieHollow" value="Feedback from Pixie Hollow">Feedback from Pixie Hollow</option>')
      }

      if (checkFormId == "chat")
      {
        document.write('<option name="chat" id="chat" value="Chat Dictionary Suggestions" selected="selected">Chat Dictionary Suggestions</option>')
      }
      else {
        document.write('<option name="chat" id="chat" value="Chat Dictionary Suggestions">Chat Dictionary Suggestions</option>')
      }

    document.write('</select>')

    check_referer()

  }
  
  function  check_referer()
  {
    var mylist=document.getElementById("Category_Message")

    searchParameters = new queryPal()
    checkFormId = searchParameters.checkQueryString("formId")

    if (checkFormId == "feedback"){
      document.getElementById("subject").value = "Pixie Hollow - " + mylist.options[4].text;
    }
    else {
      document.getElementById("subject").value = "Pixie Hollow - " + mylist.options[mylist.selectedIndex].text;
    }
  }
-->
</script>




<script language="javascript" type="text/javascript">
    var onImgArray = new Array()
  var offImgArray = new Array()
  for (i=1;i<15;i++){
    onImgArray[i] = new Image
    offImgArray[i] = new Image
  }

function imageOn(i) {
  document.images["btn" + i].src = onImgArray[i].src
}
function imageOff(i) {
  document.images["btn" + i].src = offImgArray[i].src
}

if (document.images) {

    onImgArray[1].src  = "http://a.disney.go.com/fairies/pixiehollow/images/PH_Button_okay.jpg"
    offImgArray[1].src = "http://a.disney.go.com/fairies/pixiehollow/images/PH_Button_okay.jpg"

}

function parentLoadContent(xURL, xHeader){
  parent.loadContent(xURL, xHeader)
}

function button_Back(){
  history.back(-1)
}	



  </script>

