function poprequirements()
{
launch = window.open("http://www.disneyblast.go.com/v2/help/softwarecenter.html",'launch','height=460,width=640,scrollbars=yes,resizable=no,toolbars=no')
if (parseInt(navigator.appVersion) >= 4) { launch.window.focus(); }
}

function getQueryVariable(variable) {
  var query = window.location.search.substring(1);
  var vars = query.split("&");
  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
    if (pair[0] == variable && pair[1]!="") {
      return pair[1];
    }
  }
  return "";
}

function goInstallFlash(url){

   var appRedirect = getQueryVariable("retURL");
   //document.write(url+"?retURL="+appRedirect);
   document.location.replace(url+"?retURL="+appRedirect);

}
