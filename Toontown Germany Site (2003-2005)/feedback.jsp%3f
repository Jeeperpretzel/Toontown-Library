<META content=toontown/error.tpl name=error_copy>
<SCRIPT language=javascript>


function isReady(f) {

	var isAlphaNumeric = /^[\+*\w+\s*\d*]+$/;
	var isNotNull =/.+/;
	var isNumeric =/^\d+$/;
	var isEmail =/^[\d\w\-\.]+\@[\d\w\-]+\.[\d\w]+[\.{1}[\d\w]+]*$/;
	var isValid=1;	
	var alertString='Folgende Probleme müssen behoben werden, damit wir Ihre Anmerkungen bearbeiten können:\n\n';
	if(f.subject.selectedIndex==0){
		alertString+='- Bitte die Kategorie für diese Nachricht auswählen\n';
		isValid=0;
	}	
	if (!isAlphaNumeric.test(f.Name.value)){
		alertString+='- Bitte einen Vornamen eingeben\n';
		isValid=0;
	}		
	if(!isAlphaNumeric.test(f.LastName.value)){
		alertString+='- Bitte einen Nachnamen eingeben\n';
		isValid=0;
	}
	if(!isEmail.test(f.email.value)){ 
		alertString+='- Bitte eine gültige E-Mail-Adresse eingeben\n';
		isValid=0;
	}
 	if(!isNotNull.test(f.comments.value)){
		alertString+='- Bitte Ihr Anliegen beschreiben\n';
		isValid=0;
	}
	if(isValid==1){
		return true;
	} else {
		alert(alertString);
	}
	return false;
}


