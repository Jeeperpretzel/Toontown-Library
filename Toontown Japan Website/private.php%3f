//chat情報取得処理	
function getChatInfo() {
  document.form1.action = document.form1.getul.value;
  if (document.form1.action) {
  	document.form1.submit();
  }
}

//chat情報更新処理	
function updateChatInfo() {
  document.form1.action = document.form1.upul.value;
  if (document.form1.action) {
  	document.form1.submit();
  }
}
//-->
